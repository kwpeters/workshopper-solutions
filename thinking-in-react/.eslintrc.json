{
  "env": {
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      4
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double",
      "avoid-escape"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": [
      0
    ]
  }
}
